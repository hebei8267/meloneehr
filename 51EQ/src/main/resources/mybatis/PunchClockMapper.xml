<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!-- namespace必须指向Dao接口 -->
<mapper namespace="com.tjhx.dao.affair.PunchClockMyBatisDao">
		
	<delete id="delPunchClockInfo" parameterType="com.tjhx.entity.affair.PunchClock">  
        DELETE FROM t_punch_clock WHERE clock_timey = #{clockTimeY} AND clock_timem = #{clockTimeM} AND clock_timed = #{clockTimeD};  
    </delete> 
</mapper>
